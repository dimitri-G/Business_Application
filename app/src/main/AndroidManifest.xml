<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.businessapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="开饭啦商家版"
        android:roundIcon="@drawable/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".AdviceActivity"></activity>
        <activity android:name=".ShopDetailActivity" />
        <activity android:name=".UpdateProviderActivity" />
        <activity android:name=".MyActivity" />
        <activity android:name=".CommentActivity" />
        <activity android:name=".UpdateFoodActivity" />
        <activity android:name=".CreateFoodActivity" />
        <activity android:name=".FoodDetailActivity" />
        <activity android:name=".FoodsMangerActivity" />
        <activity android:name=".OrderDetailsActivity" />
        <activity android:name=".MainMangerActivity" />
        <activity android:name=".MainOrderActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>